<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.dicodingeventaplication.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.example.dicodingeventaplication.ui.upcoming.UpcomingFragment"
        tools:layout="@layout/fragment_upcoming"/>
    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.example.dicodingeventaplication.ui.finished.FinishedFragment"
        tools:layout="@layout/fragment_finished">
    </fragment>
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.dicodingeventaplication.ui.settings.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>